{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api.py"
      }
    ],
    "env": {
      "LANGCHAIN_API_KEY": "@langchain_api_key",
      "TAVILY_API_KEY": "@tavily_api_key",
      "GOOGLE_API_KEY": "@google_api_key"
    },
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": "echo 'No build step required'",
    "outputDirectory": "."
  }